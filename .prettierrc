{
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 120,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "parser": "angular",
  "plugins": ["prettier-plugin-tailwindcss"], 
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "printWidth": 30 
      }
    }
  ]
}
